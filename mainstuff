local function QLALY_fake_script() -- Settings_2.ColourManager 
	local script = Instance.new('LocalScript', Settings_2)

	local Custom = script.Parent.CheckBox1
	local RGB = script.Parent.CheckBox2
	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	counter = 0
	while wait(0.1)do
		if (RGB.Checked.Value == true) then
			script.Parent.Parent.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
			counter = counter + 0.01
		elseif (Custom.Checked.Value == true) then
			counter = 0
			script.Parent.Parent.Parent.BackgroundColor3 = script.Parent.ColorFrame.CurrentColor.BackgroundColor3
		else
			counter = 0
			script.Parent.Parent.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.DefaultColour.Value
		end
	end
end
coroutine.wrap(QLALY_fake_script)()
local function IKYNJ_fake_script() -- ColorFrame.LocalScript 
	local script = Instance.new('LocalScript', ColorFrame)

	local Players = game:GetService("Players");local Player = Players.LocalPlayer;local Mouse = Player:GetMouse();local SG = script.Parent;local But = script.Parent.ImageButton;local Scale = 255;local Parts = 6;local R = 255;local G = 0;local B = 0;local Rx = 255;local Gx = 0;local Bx = 0;local Ry = 255;local Gy = 0;local By = 0;local Mult = script.Parent.Mult;local Current = script.Parent.CurrentColor;
	local function Color()
		local X = math.ceil((Mouse.X-But.AbsolutePosition.X)/But.AbsoluteSize.X*Scale*Parts)
		local PosX = (Mouse.X-But.AbsolutePosition.X)/But.AbsoluteSize.X
		if X < 0 then
			X = 0
		elseif X > Scale*6 then
			X = Scale*6
		end
		if X <= Scale then
			Rx = Scale
			Gx = 0
			Bx = X
		elseif X <= Scale*2 then
			Rx = Scale*2-X
			Gx = 0
			Bx = Scale
		elseif X <= Scale*3 then
			Rx = 0
			Gx = X-Scale*2
			Bx = Scale
		elseif X <= Scale*4 then
			Rx = 0
			Gx = Scale
			Bx = Scale*4-X
		elseif X <= Scale*5 then
			Rx = X-Scale*4
			Gx = Scale
			Bx = 0
		elseif X <= Scale*6 then
			Rx = Scale
			Gx = Scale*6-X
			Bx = 0
		end
		local Y = math.ceil((Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y*Scale)
		local PosY = (Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y
		if Y < 0 then
			Y = 0
		elseif Y > Scale then
			Y = Scale
		end
		Ry = Rx+math.ceil(Y/Scale*(Scale-Rx))
		Gy = Gx+math.ceil(Y/Scale*(Scale-Gx))
		By = Bx+math.ceil(Y/Scale*(Scale-Bx))
		R = math.ceil(Ry*(1-Mult.Frame.Position.Y.Scale))
		G = math.ceil(Gy*(1-Mult.Frame.Position.Y.Scale))
		B = math.ceil(By*(1-Mult.Frame.Position.Y.Scale))
		if PosX < 0 then
			PosX = 0
		elseif PosX > 1 then
			PosX = 1
		end
		if PosY < 0 then
			PosY = 0
		elseif PosY > 1 then
			PosY = 1
		end
		Current.BackgroundColor3 = Color3.fromRGB(R, G, B)
		script.Parent.ColorChanged:Fire(Color3.fromRGB(R,G,B))
	end;
	local MouseMove;
	local MouseRoll;
	But.MouseButton1Down:Connect(function()
		Color()
		MouseMove = Mouse.Move:Connect(function()
			Color()
		end)
	end);
	Mult.MouseButton1Down:Connect(function()
		local Y = (Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y
		if Y < 0 then
			Y = 0
		elseif Y > 1 then
			Y = 1
		end
		Mult.Frame.Position = UDim2.new(0,0,Y,0)
		R = math.ceil(Ry*(1-Mult.Frame.Position.Y.Scale))
		G = math.ceil(Gy*(1-Mult.Frame.Position.Y.Scale))
		B = math.ceil(By*(1-Mult.Frame.Position.Y.Scale))
		Current.BackgroundColor3 = Color3.fromRGB(R, G, B)
		MouseRoll = Mouse.Move:Connect(function()
		local Y = (Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y
		if Y < 0 then
			Y = 0
		elseif Y > 1 then
			Y = 1
		end
		Mult.Frame.Position = UDim2.new(0,0,Y,0)
		R = math.ceil(Ry*(1-Mult.Frame.Position.Y.Scale))
		G = math.ceil(Gy*(1-Mult.Frame.Position.Y.Scale))
		B = math.ceil(By*(1-Mult.Frame.Position.Y.Scale))
		Current.BackgroundColor3 = Color3.fromRGB(R, G, B)
		script.Parent.ColorChanged:Fire(Color3.fromRGB(R,G,B))
		end)
		
	end);
	Mult.MouseButton1Up:Connect(function()
		if MouseRoll ~= nil then
			MouseRoll:Disconnect()
		end
		if MouseMove ~= nil then
			MouseMove:Disconnect()
		end
	end);
	But.MouseButton1Up:Connect(function()
		if MouseRoll ~= nil then
			MouseRoll:Disconnect()
		end
		if MouseMove ~= nil then
			MouseMove:Disconnect()
		end
	end);
	Mouse.Button1Up:Connect(function()
		if MouseRoll ~= nil then
			MouseRoll:Disconnect()
		end
		if MouseMove ~= nil then
			MouseMove:Disconnect()
		end
	end);
	But.MouseLeave:Connect(function()
		if MouseRoll ~= nil then
			MouseRoll:Disconnect()
		end
		if MouseMove ~= nil then
			MouseMove:Disconnect()
		end
	end)
	Mult.MouseLeave:Connect(function()
		if MouseRoll ~= nil then
			MouseRoll:Disconnect()
		end
		if MouseMove ~= nil then
			MouseMove:Disconnect()
		end
	end)
end
