local function IVOPB_fake_script() -- ExecuteOrder77.Client 
	local script = Instance.new('LocalScript', ExecuteOrder77)

	local function getPlr(plr,args)																									--[[
		GetPlayer function by hovacmc
		(Last updated 4:24PM 24/09/2020)
		Args: (LocalPlayerName, PlayerNamesString)
		Call: getPlr(..., ...)
		
		Selectables:
		  - all/@v/@a/@e/everyone/@everyone :: Returns all players
		  - others/other/@c/@here           :: Returns other players
		  - me/@p/@s                        :: Returns executor
		  - random/rnd/@r/@random           :: Returns random player
		  - team-{teamname}                 :: Returns members of a team
		  ~ Username/Short name             ;; Returns user whos name starts with that
		
		Features:
		  - Ability to us "all", "others", "random", and more.
		  - Support for Minecraft command args "@p", "@a", etc...
		  - Ability to select teams, example: "team-green"
		  - Comma seperating support, example: "random,me,Player2,Player3"	  														]]
		local function fixTable(tableg) if table.getn(tableg) == 0 then return { nil } else return { unpack(tableg) } end end
		local function getVPlr(sc,sv,x,args,exp) for i,v in ipairs(exp) do if (sc[x] and sv[x] and args:sub(sc[x],sv[x]) == v) or (sc[x] and args:sub(sc[x]) == v) then return true end end end
		local sPlr = {} local aPlrs = { }; local oPlrs = { }; local args = tostring(args:lower())
		local plrS = game:GetService("Players"); local plr = plrS:FindFirstChild(plr)
		for idx, val in pairs(plrS:GetPlayers()) do table.insert(aPlrs,val) end
		for idx, val in pairs(plrS:GetPlayers()) do if not val.Name == plr.Name then table.insert(oPlrs,val) end end
		local function getTeams() local tsPlrs = {}; for i,v in pairs(game:GetService("Teams"):GetChildren()) do if v:IsA("Team") then table.insert(tsPlrs,v) end end return tsPlrs end
		--// Returns giving back large chunks of a server:
		if args == "all" or args == "@v" or args == "@a" or args == "@e" or args == "everyone" or args == "@everyone" then sPlr = aPlrs
		elseif args == "others" or args == "other" or args == "@c" or args == "@here" then sPlr = oPlrs else
			--// Returns giving back smaller values: (seperatable by commas)
			local sc = {1}; local sv = {}; for idx = 1, #args do if args:sub(idx,idx) == "," then table.insert(sc, idx+1) table.insert(sv,idx-1) end end
			local nrPlrs = {}	
			for x = 1, #sc do
				if (getVPlr(sc,sv,x,args,{"me", "@p", "@s"}) == true) then table.insert(nrPlrs, plr)
				elseif (getVPlr(sc,sv,x,args,{"random", "rnd", "@r", "@random"}) == true) then table.insert(nrPlrs, plrS:GetPlayers()[math.random(#aPlrs)])
				elseif (sc[x] and args:sub(sc[x]):sub(1,4):lower() == "team") then for i,v in pairs(getTeams()) do if args:sub(sc[x],sv[x]):sub(6) ~= "" and v.Name:lower():find(args:sub(sc[x],sv[x]):sub(6)) == 1 then 
							for q,p in pairs(plrS:GetPlayers()) do if p.TeamColor == v.TeamColor then table.insert(nrPlrs, p) end end end; end
					--// Username getter:	
				else for a, rPlrs in pairs(plrS:GetPlayers()) do 
						if (sc[x] and sv[x] and args:sub(sc[x],sv[x]) ~= "" and rPlrs.Name:lower():find(args:sub(sc[x],sv[x])) == 1) or (sc[x]
							and args:sub(sc[x]) ~= "" and rPlrs.Name:lower():find(args:sub(sc[x])) == 1) or (args ~= "" and rPlrs.Name:lower():find(args) == 1) then 
							table.insert(nrPlrs, rPlrs) end; end
				end; end; sPlr = nrPlrs; end; return fixTable(sPlr) end
	script.Parent.MouseButton1Down:Connect(function()
		script.Replicator:FireServer(tonumber(script.Parent.Parent.Text), getPlr(game:GetService("Players").LocalPlayer.Name,script.Parent.Parent.Parent.PlayerBox.Text))
	end)
end
coroutine.wrap(IVOPB_fake_script)()
