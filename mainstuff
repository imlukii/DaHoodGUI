local function KDYHTVB_fake_script() -- MainFrame.PageManager 
	local script = Instance.new('LocalScript', MainFrame)

	--[[----------------------------
	
	Page Manager created by hovacmc
		Version: 1b
		Updated: 2021.04.14:4.36
	
	]]------------------------------
	--[[ VARIABLES ]]--
	local pg = script.Parent.Pages
	local sb = script.Parent.SideBar
	--------------------------------
	--[[ FUNCTIONS ]]--
	local function closePages()
		for i,v in pairs(sb:GetChildren()) do
			if v:IsA('TextButton') then
				v.Open.Value = false;end
		end
		for i,v in pairs(pg:GetChildren()) do
			v.Visible = true
			local newPos = UDim2.new(0,0,-1,0)
			local Play = v:TweenPosition(
				newPos,
				Enum.EasingDirection.In,
				Enum.EasingStyle.Sine,
				2,
				true
			)
		end
	end
	local function quickClosePages()
		for i,v in pairs(sb:GetChildren()) do
			if v:IsA('TextButton') then
				v.Open.Value = false;end
		end
		for i,v in pairs(pg:GetChildren()) do
			v.Visible = true
			local newPos = UDim2.new(0,0,-1,0)
			v.Position = newPos
		end
	end
	local function openPage(v)
		closePages()
		v.Visible = true
		sb:FindFirstChild(v.Name).Open.Value = false
		local newPos = UDim2.new(0,0,0,0)
		local Play = v:TweenPosition(
			newPos,
			Enum.EasingDirection.In,
			Enum.EasingStyle.Sine,
			2,
			true
		)
	end
	--------------------------------
	--[[ PAGE MANAGER ]]--
	for i,v in pairs(sb:GetChildren()) do
		if v:IsA('TextButton') then
			v.Open.Value = false
			v.Visible = true
			v.MouseButton1Down:Connect(function()
				openPage(pg:FindFirstChild(v.Name))
			end)end
	end
	--------------------------------
	quickClosePages()
	openPage(pg:FindFirstChild('Home'))
end
coroutine.wrap(KDYHTVB_fake_script)()
local function TAVRPC_fake_script() -- MainFrame.DragScript 
	local script = Instance.new('LocalScript', MainFrame)

	--Credits go to whoever made this script.
	
	local UIS = game:GetService("UserInputService")
	function drag (Frame)
		dragToggle = nil
		dragSpeed = 0.23
		dragInput = nil
		dragStart = nil
		dragPos = nil
		function updateInput(input)
			Delta = input.Position - dragStart
			Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	drag(script.Parent)
	
end
coroutine.wrap(TAVRPC_fake_script)()
