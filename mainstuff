local function HAPR_fake_script() -- Exit.Order66 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Down:Connect(function()
		-- no references IN this code... try looking in the script attributes ;)
		local v = script.Parent.Parent.Parent
		local newPos = UDim2.new(0,0,-1,0)
		local Play = v:TweenPosition(
			newPos,
			Enum.EasingDirection.In,
			Enum.EasingStyle.Sine,
			2,
			true
		)
		wait(2)
		v.Parent:Destroy()
	end)
	-- ok i lied heres a reference: MAAAAAAAAAAAAMMA MAAAAAAMA MAMAMAMAMAMAM HOYUAUHAHAHAHUAHFIUOAHWIHI HMNANANANAMAMMA MAMAMAMMAMAMAMAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA
end
coroutine.wrap(HAPR_fake_script)()
local function OJASWKI_fake_script() -- Hide.ALittleBitOf____MixedWithSome____ ______ 
