local function AZBJYA_fake_script() -- CheckBox1.LocalScript 
	local script = Instance.new('LocalScript', CheckBox1)

	checked = "http://www.roblox.com/asset/?id=33714981"
	unchecked = "http://www.roblox.com/asset/?id=0"
	checkValue = script.Parent["Checked"]
	script.Parent.MouseButton1Down:Connect(function()
		if checkValue.Value then
			checkValue.Value = false
			script.Parent.Image = unchecked
			script.Parent.Parent.CheckBox2.Checked.Value = false
			script.Parent.Parent.CheckBox3.Checked.Value = true
		else
			checkValue.Value = true
			script.Parent.Image = checked
			script.Parent.Parent.CheckBox2.Checked.Value = false
			script.Parent.Parent.CheckBox3.Checked.Value = false
		end
	end)
	script.Parent.Checked.Changed:Connect(function()
		if checkValue.Value then
			script.Parent.Image = checked
		else
			script.Parent.Image = unchecked
		end
	end)
end
